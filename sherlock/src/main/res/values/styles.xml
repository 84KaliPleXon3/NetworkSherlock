<resources>

    <style name="SherlockAppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/sherlockPrimaryColor</item>
        <item name="colorPrimaryDark">@color/sherlockPrimaryDarkColor</item>
        <item name="colorAccent">@color/sherlockPrimaryLightColor</item>
    </style>

    <style name="SherlockAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="SherlockAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="SherlockAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
